[ignore]
.*/node_modules/immutable/dist/immutable.js.flow
.*/node_modules/fbjs/.*
.*/node_modules/draft-js/.*

[include]

[libs]

[options]
module.system=haste

module.name_mapper='^@artemkasan/draft-js-import-element$' -> '<PROJECT_ROOT>/packages/draft-js-import-element/src/main'
module.name_mapper='^@artemkasan/draft-js-utils$' -> '<PROJECT_ROOT>/packages/draft-js-utils/src/main'
module.name_mapper='^@artemkasan/synthetic-dom$' -> '<PROJECT_ROOT>/packages/synthetic-dom/src/main'

suppress_type=$FlowIssue
