{
  "name": "@artemkasan/synthetic-dom",
  "version": "1.4.0",
  "description": "Synthetic HTML DOM for use with parsing within React-RTE",
  "main": "lib/main.js",
  "module": "esm/main.js",
  "files": [
    "lib",
    "esm"
  ],
  "scripts": {
    "build:cjs": "babel src --root-mode upward --out-dir lib --ignore \"_*\"",
    "build:esm": "NODE_ENV=esm babel src --root-mode upward --out-dir esm --ignore \"_*\"",
    "build": "yarn build:cjs && yarn build:esm",
    "prepublishOnly": "yarn build"
  },
  "keywords": [
    "synthetic-dom",
    "virtual-dom",
    "markdown-parser"
  ],
  "author": "sstur@me.com",
  "license": "ISC",
  "repository": "https://github.com/artemkasan/draft-js-utils/tree/master/packages/synthetic-dom",
  "bugs": {
    "url": "https://github.com/artemkasan/draft-js-utils/issues"
  },
  "homepage": "https://github.com/artemkasan/draft-js-utils/blob/master/packages/synthetic-dom/README.md"
}
